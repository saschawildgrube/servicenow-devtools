    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.ArrayRemoveValue</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Removes a value from an array.</description>
        <mobile_callable>false</mobile_callable>
        <name>ArrayRemoveValue</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function ArrayRemoveValue(aArray, vValue)
{
	aArray = GetArrayValue(aArray);
	aArray = aArray.filter(function(element)
	{
		return element !== vValue;
	});
	return aArray;
}]]></script>
