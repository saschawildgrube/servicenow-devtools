<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Checks if a value exists in an array and returns true in that case, otherwise false.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.ArrayValueExists</name>
        <script><![CDATA[function ArrayValueExists(aArray,vValue)
{
	if (Array.isArray(aArray) == false)
	{
		return false;
	}
	for (var nIndex = 0; nIndex < aArray.length; nIndex++)
	{
		if (IsEqual(vValue,aArray[nIndex]))
		{
			return true;
		}
	}
	return false;
}]]></script>
        <script_name>ArrayValueExists</script_name>
