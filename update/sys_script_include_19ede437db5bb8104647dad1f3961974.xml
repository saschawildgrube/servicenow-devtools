        <api_name>x_snc_devtools.IsExistingRecord</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Checks if the given record is really a GlideRecord object representing an existing record.</description>
        <name>IsExistingRecord</name>
        <script><![CDATA[function IsExistingRecord(record)
{
	if (IsValidRecord(record) == false)
	{
		return false;
	}
	if (IsValidSysId(record.sys_id) == false)
	{
		return false;
	}
	return true;
}]]></script>
