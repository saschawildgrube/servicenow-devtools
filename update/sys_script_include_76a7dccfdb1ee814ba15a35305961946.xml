    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.GetAppName</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the name of a scoped application (and removes any "work in progress" postfixes). </description>
        <name>GetAppName</name>
        <script><![CDATA[function GetAppName(strApp)
{
	var grApp = GetAppRecord(strApp);
	if (IsValidRecord(grApp) == false)
	{
		return false;
	}
	var strAppName = GetStringValue(grApp.getValue('name'));
	strAppName = GetCleanAppName(strAppName);
	return strAppName;
}]]></script>
