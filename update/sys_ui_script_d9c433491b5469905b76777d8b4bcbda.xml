<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns a float value.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.GetFloatValue</name>
        <script><![CDATA[function GetFloatValue(vValue)
{
	if (vValue === true)
	{
		return 1.0;
	}
	var vResult = parseFloat(vValue);
	if (isNaN(vResult))
	{
		return 0.0;
	}
	return vResult;
}	]]></script>
        <script_name>GetFloatValue</script_name>
