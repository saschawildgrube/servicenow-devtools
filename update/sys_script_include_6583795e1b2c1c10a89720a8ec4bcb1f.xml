        <description>Returns an integer value.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetIntegerValue</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetIntegerValue(vValue)
{
	if (vValue === true)
	{
		return 1;
	}
	var vResult = parseInt(vValue);
	if (isNaN(vResult))
	{
		return 0;
	}
	return vResult;
}]]></script>
