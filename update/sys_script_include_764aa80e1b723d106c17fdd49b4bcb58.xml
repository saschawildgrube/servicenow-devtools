        <api_name>x_snc_devtools.MakeJson</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Converts an object into a JSON string.</description>
        <mobile_callable>false</mobile_callable>
        <name>MakeJson</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function MakeJson(vInput)
{
	vInput = Clone(vInput,true);
	try
	{
		var strJson = JSON.stringify(vInput,null,'\t');
	}
	catch (e)
	{
		return false;
	}
	return strJson;
}]]></script>
