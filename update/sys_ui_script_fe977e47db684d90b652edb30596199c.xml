    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns a string value in all cases. Arrays and objects are converted into comma separated strings.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.GetStringValue</name>
        <script><![CDATA[function GetStringValue(vInput)
{
	if (vInput == undefined)
	{
		return '';
	}
	if (vInput == null)
	{
		return '';
	}
	return ''+vInput;
}]]></script>
        <script_name>GetStringValue</script_name>
