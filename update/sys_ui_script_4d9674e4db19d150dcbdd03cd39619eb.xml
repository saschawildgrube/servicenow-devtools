<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns the current record's table name. This is the record displayed in a form or the record for which the list context menu was selected.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.GetCurrentRecordTableName</name>
        <script><![CDATA[function GetCurrentRecordTableName()
{
	if (IsForm())
	{
		return g_form.getTableName();
	}
	if (IsList())
	{
		return g_list.getTableName();
	}
	return '';
}]]></script>
        <script_name>GetCurrentRecordTableName</script_name>
