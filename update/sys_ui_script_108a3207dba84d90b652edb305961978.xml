<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Adds a value to an arry if it doesn't exist yet. Returns the resulting array.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.ArrayPushUnique</name>
        <script><![CDATA[function ArrayPushUnique(vArray, vValue)
{
	vArray = GetArrayValue(vArray);
	if (ArrayValueExists(vArray,vValue) == false)
	{
		vArray.push(vValue);
	}
	return vArray;
}]]></script>
        <script_name>ArrayPushUnique</script_name>
