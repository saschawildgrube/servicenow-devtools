        <api_name>x_snc_devtools.StringFindRegEx</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Finds a string in a string based on a regular expression.</description>
        <mobile_callable>false</mobile_callable>
        <name>StringFindRegEx</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function StringFindRegEx(strHaystack, strPattern, strOptions)
{
	strHaystack = GetStringValue(strHaystack);
	strPattern = GetStringValue(strPattern);
	strOptions = GetStringValue(strOptions);
	if (strPattern == '')
	{
		return -1;
	}
	return strHaystack.search(strPattern,strOptions);
}]]></script>
