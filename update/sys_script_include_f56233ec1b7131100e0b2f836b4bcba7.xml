        <api_name>x_snc_devtools.GlideRecordRestoreLocation</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Calls the function restoreLocation on the given GlideRecord object. This function is yet another cross-scope wrapper for a function that is not available in scope for no reason.</description>
        <name>GlideRecordRestoreLocation</name>
        <script><![CDATA[function GlideRecordRestoreLocation(grRecord)
{
	if (IsValidRecord(grRecord) == false)
	{
		return false;
	}
	RunScriptInGlobalScope('param1.restoreLocation();',grRecord);
	return true;
}]]></script>
