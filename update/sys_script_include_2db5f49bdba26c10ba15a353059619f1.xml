        <api_name>x_snc_devtools.IsObject</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns true if the given value is an object. False if not.</description>
        <mobile_callable>false</mobile_callable>
        <name>IsObject</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function IsObject(vInput)
{
	var strType = GetType(vInput);
	
	if (  strType == 'undefined'
	   || strType == 'null'
	   || strType == 'boolean'
	   || strType == 'number'
	   || strType == 'string'
	   || strType == 'Array')
	{
		return false;
	}
	return true;
}]]></script>
