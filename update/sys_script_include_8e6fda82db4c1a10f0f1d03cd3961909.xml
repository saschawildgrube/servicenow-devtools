        <api_name>x_snc_devtools.RedirectToUrl</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Redirects to the given url if it is valid. </description>
        <name>RedirectToUrl</name>
        <script><![CDATA[function RedirectToUrl(strUrl)
{
	strUrl = GetStringValue(strUrl);
	if (strUrl == '')
	{
		return false;
	}
	gs.setRedirect(strUrl);
	return true;
}]]></script>
