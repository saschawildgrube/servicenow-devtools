        <api_name>x_snc_devtools.UxStyleSetColor</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Sets a color in a UX Style.</description>
        <name>UxStyleSetColor</name>
        <script><![CDATA[function UxStyleSetColor(
	strSysId,
	strColorName,
	vColorValue)
{
	strColorName = GetStringValue(strColorName);
	if (strColorName == '')
	{
		return false;
	}
	var strColorValue = GetStringValue(vColorValue);
	if (strColorValue == '')
	{
		return false;
	}
	return UxStyleSetValue(strSysId,strColorValue,'base',strColorName);
}]]></script>
