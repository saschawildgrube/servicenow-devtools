        <name>Show Table Dictionary Record</name>
        <onclick/>
        <order>99998</order>
        <script><![CDATA[var grDictionary = new GlideRecord('sys_dictionary');
grDictionary.addQuery('name','=',current.name);
grDictionary.addQuery('element','=','');
grDictionary.addQuery('internal_type','=','collection');
grDictionary.query();
if (grDictionary.next())
{
	var strURL = GetInstanceRecordURI('sys_dictionary',grDictionary.sys_id);
	gs.setRedirect(strURL);	
}
else
{
	gs.addErrorMessage('Could not find the table\'s dictionary record.');
}
	
]]></script>
