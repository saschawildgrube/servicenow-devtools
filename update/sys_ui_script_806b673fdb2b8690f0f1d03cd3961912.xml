<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Opens the dialog to choose a test runner and runs a test suite.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.DoModalRunTestSuite</name>
        <script><![CDATA[function DoModalRunTestSuite(strTestSuiteSysId)
{
	var gwtm = new GwtMessage();
	var dialog = new GlideModal("atf_pick_a_browser");
	dialog.setPreference("test_suite_sys_id", ''+strTestSuiteSysId);
	dialog.setWidth("600");
	dialog.setTitle(gwtm.getMessage("Pick a Browser"));
	dialog.setPreference("focusTrap", true);
	dialog.render();
}]]></script>
        <script_name>DoModalRunTestSuite</script_name>
