        <api_name>x_snc_devtools.GetHash</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Generate a hexadecimal SHA 256 hash string. </description>
        <name>GetHash</name>
        <script><![CDATA[function GetHash(vInput)
{
	var strString = GetStringValue(vInput);
	var digest = new GlideDigest();
	return digest.getSHA256Hex(strString);
}]]></script>
