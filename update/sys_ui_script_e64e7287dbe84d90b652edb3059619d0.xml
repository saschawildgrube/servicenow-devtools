<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns true if the given value is an object. False if not.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.IsObject</name>
        <script><![CDATA[function IsObject(vInput)
{
	var strType = GetType(vInput);
	
	if (  strType == 'undefined'
	   || strType == 'null'
	   || strType == 'boolean'
	   || strType == 'number'
	   || strType == 'string'
	   || strType == 'Array')
	{
		return false;
	}
	return true;
}]]></script>
        <script_name>IsObject</script_name>
