        <value>var strLog = '';&#13;
function Log(strText)&#13;
{&#13;
	strLog = strLog + "\n" + strText;&#13;
}&#13;
(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var RenderValue = x_snc_devtools.RenderValue;&#13;
&#13;
	var bResult = true;&#13;
	&#13;
	function TestCase_IsArray(vInput, vExpected)&#13;
	{&#13;
		Log('TestCase_IsArray');&#13;
		Log('Input   : '+RenderValue(vInput));&#13;
		Log('GetType : '+RenderValue(GetType(vInput)));&#13;
		Log('Expected: '+RenderValue(vExpected));&#13;
		var vResult = IsArray(vInput);&#13;
		Log('Result  : '+RenderValue(vResult));&#13;
		if (vResult != vExpected)&#13;
		{&#13;
			bResult = false;&#13;
			Log('Test case FAILED');&#13;
		}&#13;
		Log('');&#13;
	}&#13;
	&#13;
	&#13;
	TestCase_IsArray(undefined,false);&#13;
	TestCase_IsArray(null,false);&#13;
	TestCase_IsArray(false,false);&#13;
	TestCase_IsArray(true,false);&#13;
	TestCase_IsArray(42,false);&#13;
	TestCase_IsArray('Hello World',false);&#13;
	TestCase_IsArray({},false);&#13;
	&#13;
	TestCase_IsArray([],true);&#13;
	TestCase_IsArray([''],true);&#13;
	TestCase_IsArray(['Alpha','Beta','Gamma'],true);&#13;
	&#13;
		&#13;
	stepResult.setOutputMessage(strLog);&#13;
	return bResult;&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);</value>
