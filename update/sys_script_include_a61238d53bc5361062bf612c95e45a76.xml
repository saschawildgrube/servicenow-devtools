    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.GetInstanceInfo</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns an object with the most important information about the instance.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetInstanceInfo</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetInstanceInfo()
{
	var instance = {};

	var diagnostics = GetDiagnostics();

	instance.name = GetInstanceName();
	instance.url = GetInstanceUrl();
	instance.id = diagnostics.instance_id;
	instance.version_name = GetProperty('com.glide.embedded_help.version');
	instance.version_verbose = diagnostics.instance_current_version;

	return instance;
}]]></script>
