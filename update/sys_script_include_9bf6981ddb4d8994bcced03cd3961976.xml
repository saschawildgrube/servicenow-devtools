        <api_name>x_snc_devtools.GetAppScope</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the scope name of a scoped application. </description>
        <mobile_callable>false</mobile_callable>
        <name>GetAppScope</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetAppScope(strValue)
{
	var grApp = GetAppRecord(strValue);
	if (grApp == false)
	{
		return false;
	}

	var strAppScope = GetStringValue(grApp.getValue('scope'));

	return strAppScope;
}]]></script>
