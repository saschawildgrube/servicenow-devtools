<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_include">
    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.HtmlRenderInfo</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Renders an info message in Html.</description>
        <name>HtmlRenderInfo</name>
        <script><![CDATA[function HtmlRenderInfo(strText)
{
	strText = GetStringValue(strText);
	if (strText == '')
	{
		return '';
	}
	var strHtml = '';
	strHtml += '<div class="outputmsg outputmsg_info notification notification-info">';
		strHtml += '<div class="outputmsg_text">';
			strHtml += strText;
		strHtml += '</div>';
	strHtml += '</div>';
	return strHtml;
}]]></script>
