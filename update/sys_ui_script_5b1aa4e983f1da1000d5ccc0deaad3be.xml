<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Removes a value from an array.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.ArrayRemoveValue</name>
        <script><![CDATA[function ArrayRemoveValue(aArray, vValue)
{
	aArray = GetArrayValue(aArray);
	aArray = aArray.filter(function(element)
	{
		return element !== vValue;
	});
	return aArray;
}]]></script>
        <script_name>ArrayRemoveValue</script_name>
