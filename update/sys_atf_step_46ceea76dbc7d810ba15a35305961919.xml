        <value>(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var test = new x_snc_devtools.TestAPI(outputs, steps, stepResult, assertEqual);&#13;
	&#13;
	test.SetResult(true);&#13;
&#13;
	function TestCase_HttpRequest(vInput, vExpected)&#13;
	{&#13;
		function TestCase_HttpRequest_CheckResult(vInput,vExpected,vResult)&#13;
		{&#13;
			if (vResult != vExpected)&#13;
			{&#13;
				var strResult = GetStringValue(vResult);&#13;
				if (strResult.includes(vExpected) == false)&#13;
				{&#13;
					return false;&#13;
				}&#13;
				return true;&#13;
			}&#13;
			return (vExpected == vResult);&#13;
		}&#13;
&#13;
		test.TestCase(&#13;
			'Function HttpRequest()',&#13;
			HttpRequest,&#13;
			{&#13;
				'Input': vInput&#13;
			},&#13;
			vExpected,&#13;
			TestCase_HttpRequest_CheckResult);&#13;
	}&#13;
&#13;
	TestCase_HttpRequest('',false);&#13;
	TestCase_HttpRequest('http://www.example.com','&lt;title&gt;Example Domain&lt;/title&gt;');&#13;
	TestCase_HttpRequest(GetInstanceUrl(),'window.NOW.');	&#13;
&#13;
	&#13;
	return test.GetResult();&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);&#13;
