        <api_name>x_snc_devtools.GetFloatValue</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns a float value.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetFloatValue</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetFloatValue(vValue)
{
	if (vValue === true)
	{
		return 1.0;
	}
	var vResult = parseFloat(vValue);
	if (isNaN(vResult))
	{
		return 0.0;
	}
	return vResult;
}]]></script>
