        <api_name>x_snc_devtools.TimeAddDays</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Adds (or substracts) a given number of days to (or from) a given time value (in milliseconds).</description>
        <name>TimeAddDays</name>
        <script><![CDATA[function TimeAddDays(nTime, nDays)
{
	nTime = GetIntegerValue(nTime);
	nDays = GetIntegerValue(nDays);
	nTime += nDays * 1000 * 60 * 60 * 24;
	return nTime;
}]]></script>
