    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.StringFind</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the index of the needle string within the haystack string, -1 if the needle is not contained in the haystack. </description>
        <name>StringFind</name>
        <script><![CDATA[function StringFind(strHaystack, strNeedle)
{
	strHaystack = GetStringValue(strHaystack);
	strNeedle = GetStringValue(strNeedle);
	if (strNeedle == '')
	{
		return -1;
	}
	return strHaystack.indexOf(strNeedle);
}]]></script>
