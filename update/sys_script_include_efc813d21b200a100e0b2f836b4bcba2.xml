        <api_name>x_snc_devtools.StringMatchRegEx</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the first matching string based on the given input string and the regular expression. The function returns false if no matching string could be found.</description>
        <name>StringMatchRegEx</name>
        <script><![CDATA[function StringMatchRegEx(strInput, strRegEx)
{
	strInput = GetStringValue(strInput);
	var aMatches = strInput.match(strRegEx);

	if (aMatches)
	{
		return GetValue(aMatches,1);
	}
	return false;
}]]></script>
