        <value>var strLog = '';&#13;
function Log(strText)&#13;
{&#13;
	strLog = strLog + "\n" + strText;&#13;
}&#13;
(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var bResult = true;&#13;
	&#13;
	function TestCase_GetStringValue(vInput, vExpected)&#13;
	{&#13;
		Log('TestCase_GetStringValue');&#13;
		Log('Input: '+RenderValue(vInput));&#13;
		Log('Expected: '+vExpected);&#13;
		var vResult = GetStringValue(vInput);&#13;
		Log('Result  : '+vResult);&#13;
		if (vResult !== vExpected)&#13;
		{&#13;
			bResult = false;&#13;
			Log('Test case FAILED');&#13;
		}&#13;
		Log('');&#13;
	}&#13;
&#13;
	TestCase_GetStringValue('','');&#13;
	TestCase_GetStringValue(undefined,'');&#13;
	TestCase_GetStringValue(null,'');&#13;
	TestCase_GetStringValue(0,'0');&#13;
	TestCase_GetStringValue(10,'10');&#13;
	TestCase_GetStringValue(3.14,'3.14');&#13;
	TestCase_GetStringValue(false,'false');&#13;
	TestCase_GetStringValue(true,'true');&#13;
	TestCase_GetStringValue('Alpha','Alpha');&#13;
	TestCase_GetStringValue(['Alpha','Beta'],'Alpha,Beta');&#13;
		&#13;
	stepResult.setOutputMessage(strLog);&#13;
	return bResult;&#13;
	&#13;
