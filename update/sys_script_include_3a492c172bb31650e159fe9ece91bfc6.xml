        <api_name>x_snc_devtools.GlideRecordSetAbortAction</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Executes the setAbortAction member function of a glide record in global scope.</description>
        <mobile_callable>false</mobile_callable>
        <name>GlideRecordSetAbortAction</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GlideRecordSetAbortAction(grRecord,bAbort)
{
	if (IsValidRecord(grRecord) == false)
	{
		return false;
	}
	
	RunScriptInGlobalScope(
		'param1.setAbortAction(param2);',grRecord,GetBoolValue(bAbort));
	
	return true;	
}]]></script>
