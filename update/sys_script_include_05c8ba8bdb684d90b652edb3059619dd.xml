        <api_name>x_snc_devtools.ArrayPushUnique</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Adds a value to an arry if it doesn't exist yet. Returns the resulting array.</description>
        <name>ArrayPushUnique</name>
        <script><![CDATA[function ArrayPushUnique(vArray, vValue)
{
	vArray = GetArrayValue(vArray);
	if (ArrayValueExists(vArray,vValue) == false)
	{
		vArray.push(vValue);
	}
	return vArray;
}]]></script>
