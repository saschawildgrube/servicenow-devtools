	&#13;
	test.SetResult(true);&#13;
&#13;
	function TestCase_CreateTestUser(strName, vExpected)&#13;
	{&#13;
		function TestCase_CreateTestUser_CheckResult(vInput,vExpected,vResult)&#13;
		{&#13;
			if (vExpected == false)&#13;
			{&#13;
				return (vExpected == vResult);&#13;
			}&#13;
			if (IsValidSysId(vResult) == false)&#13;
			{&#13;
