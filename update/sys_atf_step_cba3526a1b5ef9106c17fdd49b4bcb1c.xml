			test.Log('Created User: '+RenderValue(GetUserRecord(vResult),true));&#13;
						&#13;
			return true;&#13;
		}&#13;
&#13;
		test.TestCase(&#13;
			'Function CreateTestUser()',&#13;
			CreateTestUser,&#13;
			{&#13;
				'Name': strName&#13;
			},&#13;
			vExpected,&#13;
			TestCase_CreateTestUser_CheckResult);&#13;
	}&#13;
&#13;
	TestCase_CreateTestUser(null,false);&#13;
	TestCase_CreateTestUser('DevTools Test ATF',true);&#13;
	TestCase_CreateTestUser('devtools.test.atf',false);&#13;
	&#13;
	return test.GetResult();&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);</value>
