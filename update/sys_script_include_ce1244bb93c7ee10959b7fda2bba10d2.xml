    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.NightlyQA</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Runs all CodeSanity checks (if available) and all default Test Suites of the given application and all of its dependencies. </description>
        <mobile_callable>false</mobile_callable>
        <name>NightlyQA</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function NightlyQA(vApp)
{
	var grApp = GetAppRecord(vApp);
	if (IsValidRecord(grApp) == false)
	{
		return false;
	}
	var results = {};
	if (IsFunction(x_snc_codesanity.ScanAppWithDependencies) == true)
	{
		results.scan = x_snc_codesanity.ScanAppWithDependencies(grApp,true);
	}
	results.test = x_snc_devtools.RunDefaultTestSuitesWithDependencies(grApp);
	return results;
}]]></script>
