<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Replaces all occurrences of a string by another string in a string.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.StringReplace</name>
        <script><![CDATA[function StringReplace(strHaystack,strNeedle,strReplace)
{
	strHaystack = GetStringValue(strHaystack);
	return strHaystack.replaceAll(strNeedle,strReplace);
}]]></script>
        <script_name>StringReplace</script_name>
